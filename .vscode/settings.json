{
    "python.languageServer": "Pylance",
    "files.associations": {
        "**/*.html": "html",
        "**/templates/**/*.html": "django-html",
        "**/templates/**/*": "django-txt"
    },
    "emmet.includeLanguages": {
        "django-html": "html",
    },
    "[html]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
      }

}